[
  {
    "kernelSource": {
      "name": "Perf-Kernel",
      "repo": "https://github.com/Lafactorial/tissot_kernel",
      "branch": "lineage-21",
      "device": "tissot",
      "defconfig": "tissot_defconfig"
    },
    "toolchains": [
      {
        "repo": "https://github.com/Lafactorial/android_prebuilts_clang_host_linux-x86_clang-r416183d",
        "branch": "11.0",
        "name": "clang",
        "binaryEnv": [
          "bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "bin/clang",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "LDGOLD": "aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis",
        "CONFIG_THINLTO": ""
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "custom": {
        "repo": "https://github.com/Lafactorial/Anykernel3/",
        "branch": "lineage-21"
      }
    },
    "enableKernelSU": false,
    "enableLXC": true
  }
]
